  },
  "homepage": "https://github.com/herka10/Assignmint#readme",
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20"
  },
  "dependencies": {
    "if-env": "^1.0.4"
  }
}
